<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MovesBar">
        <attr name="startStandSide" format="reference"/>
    </declare-styleable>
    
    <declare-styleable name="OneMoveBar">
        <attr name="startSide" format="enum">
            <enum name="upperRight" value="0"/>
            <enum name="upperLeft" value="1"/>
            <enum name="lowerLeft" value="2"/>
            <enum name="lowerRight" value="3"/>
        </attr>
        <attr name="endSide" format="enum">
            <enum name="upperRight" value="0"/>
            <enum name="upperLeft" value="1"/>
            <enum name="lowerLeft" value="2"/>
            <enum name="lowerRight" value="3"/>
        </attr>
    </declare-styleable>

</resources>